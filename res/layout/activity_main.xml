<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gotchaninja"
    tools:context=".MainActivity" >

	<Spinner
	    android:id="@+id/spinnerFirst"
	    style="?android:attr/spinnerItemStyle"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView5"
	    android:layout_alignRight="@+id/targetName"
	    android:layout_below="@+id/textView5"
	    android:layout_marginTop="19dp"
	    android:gravity="center"
	    android:textColor="#000000" />

	<TextView
	    android:id="@+id/textView3"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/yourName"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="33dp"
	    android:text="@string/targetText"
	    android:textColor="#000000"
	    android:textSize="25sp" />

	<TextView
	    android:id="@+id/yourName"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentTop="true"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="28dp"
	    android:text="@string/name"
	    android:textColor="#000000"
	    android:textSize="25sp" />

	<TextView
	    android:id="@+id/targetName"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/textView3"
	    android:layout_centerHorizontal="true"
	    android:text="@string/targetName"
	    android:textColor="#000000"
	    android:textSize="25sp" />

	<Button
	    android:id="@+id/button1"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/spinnerFirst"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="17dp"
	    android:onClick="sendMessage"
	    android:text="Go!"
	    android:textColor="#000000" />

	<TextView
	    android:id="@+id/textView5"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView3"
	    android:layout_alignRight="@+id/textView3"
	    android:layout_below="@+id/targetName"
	    android:layout_marginTop="32dp"
	    android:text="@string/targetselectText"
	    android:textColor="#000000"
	    android:textSize="15sp" />

	<TextView
	    android:id="@+id/textView6"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/spinnerFirst"
	    android:layout_below="@+id/button1"
	    android:text="getOut" />

	<TextView
	    android:id="@+id/textView7"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView6"
	    android:layout_below="@+id/textView6"
	    android:text="getTotal" />
	
	<TextView
	    android:id="@+id/textView8"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView7"
	    android:layout_below="@+id/textView7"
	    android:text="percentOut" />

	<TextView
	    android:id="@+id/textView9"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView8"
	    android:layout_below="@+id/textView8"
	    android:text="numberTagged" />

	<TextView
	    android:id="@+id/textView10"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView9"
	    android:layout_below="@+id/textView9"
	    android:text="timeStamp" />

	<TextView
	    android:id="@+id/textView11"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView10"
	    android:layout_below="@+id/textView10"
	    android:text="topTen" />

	<TableLayout
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/textView11"
	    android:layout_below="@+id/textView11" >

	    <TableRow
	        android:id="@+id/tableRow1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
	    </TableRow>

	    <TableRow
	        android:id="@+id/tableRow2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
	    </TableRow>

	    <TableRow
	        android:id="@+id/tableRow3"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
	    </TableRow>

	    <TableRow
	        android:id="@+id/tableRow4"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
	    </TableRow>
	</TableLayout>
	
</RelativeLayout>